{
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es2015", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny":true,
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types",
      "./public/node_modules/@types"
    ]
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "./node_modules",
    "./public/node_modules"
  ]
}